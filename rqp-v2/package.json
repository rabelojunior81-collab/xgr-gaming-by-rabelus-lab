{
  "name": "@rqp/core",
  "version": "2.0.0",
  "description": "Rabelus Quant Protocol - Core Framework (Simplified)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
